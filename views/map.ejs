<div class="row">
	<div class="hidden-xs hidden-sm col-md-12">
		<h3>Map</h3>
		<svg id="map" width="640" height="640"></svg>
	</div>
	<div class="col-xs-12">
		<h3>Table</h3>
		<table>
		<% for(var i = 0, l = data.length; i < l; i++) { %>
		<% var vote = data[i].votes() %>
			<tr>
		    	<td><%= data[i].id %></td>
		    	<td><%= data[i].name %></td>
		    	<td>
		    		<% for(var j = 0, k = vote.length; j < k; j++) { %>
		    		<%= vote[j] %><br>
		    		<% } %>
		    	</td>
			</tr>
		<% } %>
		</table>
	</div>
</div>
<% block('localScripts', '<script src="custom-js/map.js"></script>') %>